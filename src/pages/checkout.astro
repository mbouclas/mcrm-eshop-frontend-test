---
import Layout from '@layouts/Layout.astro';

import Checkout from '@components/checkout/main.svelte';


---

<Layout title="Checkout" >

  <div class="bg-gray-50">

    <main class="mx-auto max-w-7xl px-4 pb-24 pt-16 sm:px-6 lg:px-8">
      <div class="mx-auto max-w-2xl lg:max-w-none">
        <h1 class="sr-only">Quotation</h1>
        <h1 class="text-xl lg:text-2xl font-semibold mb-6">Quotation</h1>
        <Checkout client:load />

      </div>
    </main>

  </div>
</Layout>
